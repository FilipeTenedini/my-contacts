{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": "off",
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off"
    }
}
